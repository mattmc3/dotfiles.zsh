# macOS
ohmyzsh/ohmyzsh path:plugins/iterm2 conditional:is-macos
zshzoo/macos conditional:is-macos

# Prompt
sindresorhus/pure kind:fpath
romkatv/powerlevel10k kind:fpath

# Regular plugins
mattmc3/zman

